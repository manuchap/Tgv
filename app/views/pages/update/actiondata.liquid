---
title: Action data
published: true
handle: actiondata
response_type: application/json
---
{% with_scope _slug: params.slug %}
{% for action in contents.actions %}
{
    {% include 'json', key: 'name', value: action.name %},
    {% include 'json', key: 'desc', value: action.desc %},
    "code": "{{ action.code | multi_line | replace: '"', ''' }}"
}
{% endfor %}
{% endwith_scope %}