---
title: CreateApp
published: true
handle: create_app
---
{% action 'createApp' %}
var params = getProp('params');
{% case wagon %}
{% when true %}
createEntry('apps', { name: params.name, desc: params.desc, pict: params.pict, url: params.url, demo: params.demo, account_id: params.account });
{% else %}
createEntry('apps', { name: params.name, desc: params.desc, pict: params.pict, url: params.url, demo: params.demo, account: params.account });
{% endcase %}
redirectTo('me');
{% endaction %}
