---
title: CreateMisc
published: true
handle: create_misc
---
{% action 'createMisc' %}
var params = getProp('params');
createEntry('miscs', { name: params.name, desc: params.desc, type: params.type, code: params.code.replace(/\n/g, "<br>"), account: params.account });
redirectTo('me');
{% endaction %}
